{
    "liveSassCompile.settings.formats": [
      {
        "format": "expanded",
        "extensionName": ".css",
        "savePath": "/src/css",
        "savePathReplacementPairs": null
      },
      {
        "format": "compressed",
        "extensionName": ".min.css",
        "savePath": "/src/css",
        "savePathReplacementPairs": null
      }
    ],
    "liveSassCompile.settings.excludeList": [
      "//node_modules/",
      "/.vscode/**"
